{
  "name": "@tronweb3/tronwallet-adapters",
  "version": "1.2.17",
  "description": "Wallet adapters to help developers interact with Tron wallets using consistent API.",
  "keywords": [
    "TRON",
    "TronWeb",
    "adapters"
  ],
  "author": "tronweb3",
  "repository": {
    "type": "git",
    "url": "https://github.com/tronweb3/tronwallet-adapter"
  },
  "homepage": "https://walletadapter.org",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  },
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/types/index.d.ts",
  "exports": {
    "require": "./lib/cjs/index.js",
    "import": "./lib/esm/index.js",
    "types": "./lib/types/index.d.ts"
  },
  "files": [
    "lib",
    "src",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@tronweb3/tronwallet-adapter-binance": "^1.0.2",
    "@tronweb3/tronwallet-adapter-bitkeep": "^1.1.7",
    "@tronweb3/tronwallet-adapter-foxwallet": "^1.0.1",
    "@tronweb3/tronwallet-adapter-bybit": "^1.0.2",
    "@tronweb3/tronwallet-adapter-gatewallet": "^1.0.4",
    "@tronweb3/tronwallet-adapter-guarda": "^1.0.0",
    "@tronweb3/tronwallet-adapter-imtoken": "^1.0.3",
    "@tronweb3/tronwallet-adapter-tronlink": "^1.1.13",
    "@tronweb3/tronwallet-adapter-ledger": "^1.1.11",
    "@tronweb3/tronwallet-adapter-walletconnect": "^3.0.1",
    "@tronweb3/tronwallet-adapter-tomowallet": "^1.0.1",
    "@tronweb3/tronwallet-adapter-trust": "^1.0.2",
    "@tronweb3/tronwallet-adapter-okxwallet": "^1.0.7",
    "@tronweb3/tronwallet-adapter-tokenpocket": "^1.0.8"
  },
  "devDependencies": {
    "@walletconnect/modal": "^2.6.2",
    "@walletconnect/sign-client": "^2.1.4",
    "happy-dom": "20.0.10",
    "shx": "^0.3.4",
    "vitest": "^2.0.4"
  },
  "scripts": {
    "test": "vitest run",
    "clean": "shx mkdir -p lib && shx rm -rf lib",
    "package": "shx echo '{ \"type\": \"commonjs\" }' > lib/cjs/package.json",
    "build:umd": "node ../../../scripts/build-umd.js"
  }
}
{
  "name": "@tronweb3/tronwallet-adapter-tomowallet",
  "version": "1.0.1",
  "description": "Wallet adapter for Tomo Wallet app.",
  "keywords": [
    "TRON",
    "TronWeb",
    "Tomo Wallet"
  ],
  "author": "Tomo Inc.",
  "repository": {
    "type": "git",
    "url": "https://github.com/tronweb3/tronwallet-adapter"
  },
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  },
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/types/index.d.ts",
  "exports": {
    "require": "./lib/cjs/index.js",
    "import": "./lib/esm/index.js",
    "types": "./lib/types/index.d.ts"
  },
  "files": [
    "lib",
    "src",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@tronweb3/tronwallet-abstract-adapter": "^1.1.9",
    "@tronweb3/tronwallet-adapter-tronlink": "^1.1.13"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.20.0",
    "jest": "29.6.4",
    "jest-environment-jsdom": "^29.6.4",
    "shx": "^0.3.4"
  },
  "scripts": {
    "clean": "shx mkdir -p lib && shx rm -rf lib",
    "package": "shx echo '{ \"type\": \"commonjs\" }' > lib/cjs/package.json",
    "test": "jest",
    "test:coverage": "jest  --coverage",
    "build:umd": "node ../../../scripts/build-umd.js"
  }
}